{
  "name": "biller-api",
  "version": "0.0.1",
  "description": "API for the biller app",
  "main": "index.js",
  "author": "Exponents",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node build/index.js",
    "dev": "nodemon",
    "build": "swc src -d build",
    "test": "jest --passWithNoTests",
    "test-dev": "jest --passWithNoTests --setupFiles dotenv/config",
    "prepare": "husky install",
    "lint": "eslint",
    "pre-commit": "yarn lint && yarn test-dev && yarn build",
    "post-merge": "yarn install",
    "commit-msg": "npx --no-install commitlint --edit"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.0",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.4",
    "morgan": "^1.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.27",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/express-fileupload": "^1.4.1",
    "@types/jest": "^29.2.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
